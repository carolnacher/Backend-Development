
 <h1><%= title %></h1>
 <%- messages() %>
 <% if (typeof errors !== 'undefined' && errors) { %>
  <ul class="notice">
    <% errors.array().forEach(error => { %>
      <li><%= error.msg %></li>
    <% }) %>
  </ul>
<% } %>

  <section class="management-options">
    <ul>
      <li><a href="/inv/addClassification" title="Add a new classification">Add New Classification</a></li>
      <li><a href="/inv/newVehicle" title="Add a new vehicle">Add New Vehicle</a></li>
    </ul>
  </section>

  <section class="inventory-display">
    <h2>Inventory by Classification</h2>
    <form id="classificationForm">
      <label for="classificationList">Select Classification:</label>
      <%- classificationSelect %>
    </form>

<noscript>JavaScript must be enabled to use this page.</noscript>
<script src="/js/inventory.js"></script>

   